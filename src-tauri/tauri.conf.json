{
  "productName": "tauri-kit",
  "version": "0.1.0",
  "identifier": "prabhu.corgi.app",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:8080",
    "beforeDevCommand": "pnpm  dev",
    "beforeBuildCommand": "pnpm  build"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-chat",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}